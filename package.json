{
    "name": "storagecontrol",
    "version": "1.0.0",
    "description": "My Electron application description",
    "author": "raul",
    "scripts": {
        "lint": "echo \"No linting configured\"",
        "css-build": "node-sass --omit-source-map-url src/public/css/app.scss src/public/css/app.css",
        "css-watch": "npm run css-build -- --watch",
        "make": "electron-forge make",
        "package": "electron-forge package",
        "publish": "electron-forge publish",
        "start": "electron-forge start",
        "start-css": "npm run css-watch",
        "migrations-run": "cd ./database && ../node_modules/knex/bin/cli.js migrate:latest",
        "migrations-rollback": "cd ./database && ../node_modules/knex/bin/cli.js migrate:rollback"
    },
    "dependencies": {
        "bulma": "^0.7.2",
        "electron-compile": "^6.4.3",
        "electron-devtools-installer": "^2.2.4",
        "electron-squirrel-startup": "^1.0.0",
        "knex": ">=0.19.5",
        "mysql": "^2.16.0",
        "node-sass": "^4.13.0",
        "vue": "^2.6.11",
        "vue-router": "^3.1.3",
        "vue-style-loader": "^4.1.2",
        "vue-template-compiler": "^2.6.11",
        "vueify": "^9.4.1",
        "vuetify": "^1.5.21",
        "vuex": "^3.1.2",
        "vuex-electron": "^1.0.3"
    },
    "devDependencies": {
        "babel-plugin-transform-async-to-generator": "^6.24.1",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "electron-forge": "^5.2.2",
        "electron-prebuilt-compile": "3.0.6",
        "stylus": "^0.54.7",
        "stylus-loader": "^3.0.1",
        "vue-cli-plugin-vuetify": "^0.4.6",
        "vuetify-loader": "^1.4.3"
    },
    "config": {
        "forge": {
            "make_targets": {
                "win32": [
                    "squirrel"
                ],
                "darwin": [
                    "zip"
                ],
                "linux": [
                    "deb",
                    "rpm"
                ]
            },
            "electronPackagerConfig": {
                "packageManager": "npm"
            },
            "electronWinstallerConfig": {
                "name": "storagecontrol"
            },
            "electronInstallerDebian": {},
            "electronInstallerRedhat": {},
            "github_repository": {
                "owner": "",
                "name": ""
            },
            "windowsStoreConfig": {
                "packageName": "",
                "name": "storagecontrol"
            }
        }
    },
    "keywords": [],
    "license": "MIT",
    "main": "src/front/index.js",
    "productName": "storagecontrol"
}
